@{
    Layout = "~/Views/Shared/_UsuarioLayout.cshtml";
}
@model Pedido

<div class="container text-center">


    <div class="form-group">
        <div class="row">
            <label>
                El pedido " @Model.NombreNegocio " se inicio correctamente<br />

            </label>
        </div>
    </div>
    <br />


    <form action="/Pedidos/Editar/@Model.IdPedido">
        <button class="btn btn-warning my-2 my-sm-0" type="submit">Editar</button>
    </form>
</div>

